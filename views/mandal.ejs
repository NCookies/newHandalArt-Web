		<div class="mandal-container">
		<div class="mandal-view">
			<div class="view-search">
				<input st-search="" class="form-control" placeholder="만다라트 검색" type="text"/>
				<button type="button" class="glyphicon glyphicon-plus"></button>
			</div>
			<div class="view-box">
				<div class="box-object">
					<div class="box-title">
						5.five
					</div>
					<div class="box-date">
						date
					</div>
					<div class="box-content">
						contentzxcontentzxcontentzxcontentzxcontentzxcontentzxcontentzxcontentzxcontentzx
