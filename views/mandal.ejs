						date
					</div>
					<div class="box-content">
						content
					</div>
				</div>
			</div>
		</div>	

        <div class="mandal-mandal">
			<div class="mandal-pallet">
			</div> 
			<div class="mandal-table">
				<h1 class="title" href="#" editable-text="user.title">{{ user.title || '만다라트 이름' }}</h1>
				<h2 class="description" href="#" editable-textarea="user.desc" e-rows="7" e-cols="20">
					<pre>{{ user.desc || '만다라트 상세설명' }}</pre></h2>

				<table class="table table-bordered">
					<tbody>
						<tr ng-repeat="row in mandal">
